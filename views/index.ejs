            <tr>
              <td>2</td>
              <td>46:A6:BB:C2:D3:AA</td>
              <td>2</td>
              <td>1.0</td>
              <td>off</td>
            </tr>
            <tr>
              <td>3</td>
              <td>1A:02:9F:C7:43:AC</td>
              <td>3</td>
              <td>1.0</td>
              <td>off</td>
            </tr>
          </table>
        </div>
        <div id="logs" class="admin_stuff"></div>
      </div>
    </div>
  </div>
  </body>
</html>
